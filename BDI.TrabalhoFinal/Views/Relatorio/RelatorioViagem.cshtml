@{
    ViewData["Title"] = "Relatório de Viagens";
    var viagens = Model as IEnumerable<dynamic>;
}

<h2>Relatório de Viagens</h2>

<table>
    <thead>
        <tr>
            <th>Marca</th>
            <th>Placa</th>
            <th>Local Origem</th>
            <th>Local Destino</th>
            <th>Nome Motorista</th>
            <th>Nome Passageiro</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var viagem in viagens)
        {
            <tr>
                <td>@viagem.Marca</td>
                <td>@viagem.Placa</td>
                <td>@viagem.LocalOrigem</td>
                <td>@viagem.LocalDestino</td>
                <td>@viagem.NomeMotorista</td>
                <td>@viagem.NomePassageiro</td>
            </tr>
        }
    </tbody>
</table>

